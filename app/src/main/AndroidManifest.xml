        android:largeHeap="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:icon,android:theme">
        <activity android:name=".Youtube"
            android:screenOrientation="landscape"
            ></activity>
        <activity android:name=".Chakra_meditation" />
        <activity android:name=".login_fragment" />
        <activity android:name=".login_page" />
        <activity android:name=".spalsh_activity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".environment_list" />
        <activity android:name=".med_channel_list.channel2_med_list" />
        <activity android:name=".med_channel_list.channel1_med_list" />
        <activity android:name=".channel_yoga_list.yoga_channel1_list" />
        <activity android:name=".channel_yoga_list.yoga_channel2_list" />
